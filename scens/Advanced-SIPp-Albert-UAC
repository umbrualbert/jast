<!--                                                                    -->
<!--                 Sipp 'uac' scenario with pcap (rtp) play           -->
<!--                                                                    -->

<scenario name="UAC with media">
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->

  <!--
  SIP Messages
  ============

  <-Rx-- INVITE
  --Tx-> 100
  --Tx-> 183
  <-Rx-- PRACK
  --Tx-> 200 (PRACK)
  ------ Pause[ms]=350
  --Tx-> 200 (INVITE)
  <-Rx-- ACK
  ------ Play pcap=<test_file..pcap
  ------ Pause[ms]=10000
  --Tx-> UPDATE
  ------ Play pcap=<test_file..pcap
  <-Rx-- 200 (UPDATE)
  <-Rx-- BYE
  --Tx-> 200 (BYE)
  -->

  <send retrans="500">
    <![CDATA[

      INVITE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:[field1]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>
      Call-ID: [branch][pid][call_id][clock_tick]
      CSeq: 1 INVITE
      Contact: sip:[field1]@[local_ip]:[local_port]
      Max-Forwards: 70
      User-Agent: SIPp UAC
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[local_ip_type] [local_ip]
      t=0 0
      m=audio [auto_media_port] RTP/AVP 18 101
      a=rtpmap:18 G729/8000
      a=fmtp:18 annexb=no
      a=rtpmap:101 telephone-event/8000
      a=fmtp:101 0-11,16

    ]]>
  </send>

  <recv response="100" optional="true">
  </recv>

  <recv response="183" next="3">
  </recv>

  <!--
  Send Provisional Ack
  Change CSeq and add To-tag received on 18x
  -->
<label id="3"/>

  <send start_txn="prack" retrans="500">

     <![CDATA[

      PRACK sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:[field1]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[peer_tag_param]
      [last_Call-ID:]
      CSeq: 2 PRACK
      Contact: sip:[field1]@[local_ip]:[local_port]
      Max-Forwards: 70
      User-Agent: SIPp UAC
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: 0


    ]]>
  </send>


  <!-- By adding rrs="true" (Record Route Sets), the route sets         -->
  <!-- are saved and used for following messages sent. Useful to test   -->
  <!-- against stateful SIP proxies/B2BUAs.                             -->

  <!-- <recv response="200" rtd="true" crlf="true">                     -->
  <recv response="200" response_txn="prack" rtd="true" crlf="true">
  </recv>

  <!-- Early media from server for 183 (with SDP)                        -->
  <!-- Pause 90 seconds, which is approximately the duration of the      -->
  <!-- PCAP file                                                        -->
  <pause milliseconds="40000"/>


  <recv response="200" rtd="true" crlf="true">
  </recv>

  <!-- Packet lost can be simulated in any send/recv message by         -->
  <!-- by adding the 'lost = "10"'. Value can be [1-100] percent.       -->
  <send>
    <![CDATA[

      ACK sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:[field1]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[peer_tag_param]
      [last_Call-ID:]
      CSeq: 1 ACK
      Contact: sip:[field1]@[local_ip]:[local_port]
      Max-Forwards: 70
      User-Agent: SIPp UAC
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <!-- Play a pre-recorded PCAP file (RTP stream)                       -->
  <nop>
    <action>
      <exec play_pcap_audio="scens/G729-1Minute30Seconds.pcap"/>
    </action>
  </nop>

  <!-- Pause 90 seconds, which is approximately the duration of the      -->
  <!-- PCAP file                                                        -->
<pause milliseconds="90000"/>

  <recv request="UPDATE">
    <action>
      <ereg regexp="[Bb]ranch=(.*)" search_in="hdr" header="Via:" check_it="true" assign_to="UPDATEbranch" />
      <ereg regexp=".*" search_in="hdr" header="CSeq:" check_it="true" assign_to="UPDATEcseq" />
    </action>
  </recv>

  <!--
  200 OK response (UPDATE with SDP)......
  -->
    <send>
      <![CDATA[

        SIP/2.0 200 OK
        Via: SIP/2.0/[transport] [remote_ip]:[remote_port];[$UPDATEbranch]
        [last_From:]
        [last_To:]
        [last_Call-ID:]
        User-Agent: SIPp UAC
        CSeq: [$UPDATEcseq]
        Content-Length: [len]

        v=0
        o=CiscoSystemsSIP-GW-UserAgent 4723 4841 IN IP4 [local_ip]
        s=SIP Call
        c=IN IP4 [local_ip]
        t=0 0
        m=audio [media_port] RTP/AVP 8 101
        c=IN IP4 [local_ip]
        m=audio [auto_media_port] RTP/AVP 18 101
        a=rtpmap:18 G729/8000
        a=fmtp:18 annexb=no
        a=rtpmap:101 telephone-event/8000
        a=fmtp:101 0-15
        a=ptime:20

      ]]>
    </send>

    <nop>
      <action>
        <exec play_pcap_audio="scens/G729-2minute-call-flow.pcap"/>
      </action>
    </nop

  <pause milliseconds="130000"/>

  <!-- The 'crlf' option inserts a blank line in the statistics report. -->
  <send retrans="500">
    <![CDATA[

      BYE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:[field1]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[peer_tag_param]
      [last_Call-ID:]
      CSeq: 2 BYE
      Contact: sip:[field1]@[local_ip]:[local_port]
      Max-Forwards: 70
      User-Agent: SIPp UAC
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" crlf="true">
  </recv>

  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10, 50, 100, 500, 1000, 5000, 10000"/>

</scenario>
